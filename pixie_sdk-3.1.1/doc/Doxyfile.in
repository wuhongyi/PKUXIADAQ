# Doxyfile 1.9.1
#---------------------------------------------------------------------------
# Project related configuration options
#---------------------------------------------------------------------------
DOXYFILE_ENCODING      = UTF-8
PROJECT_NAME           = "@CMAKE_PROJECT_NAME@"
PROJECT_NUMBER         =
PROJECT_BRIEF          =
PROJECT_LOGO           =
OUTPUT_DIRECTORY       = "@CMAKE_CURRENT_BINARY_DIR@/"
ALLOW_UNICODE_NAMES    = NO
BRIEF_MEMBER_DESC      = YES
FULL_PATH_NAMES        = NO
SEPARATE_MEMBER_PAGES  = YES
TAB_SIZE               = 4
MARKDOWN_SUPPORT       = YES
TOC_INCLUDE_HEADINGS   = 5
AUTOLINK_SUPPORT       = YES
BUILTIN_STL_SUPPORT    = YES
CPP_CLI_SUPPORT        = YES
#---------------------------------------------------------------------------
# Build related configuration options
#---------------------------------------------------------------------------
EXTRACT_ALL            = YES
EXTRACT_STATIC         = YES
EXTRACT_LOCAL_METHODS  = YES
CASE_SENSE_NAMES       = YES
LAYOUT_FILE            =
#---------------------------------------------------------------------------
# Configuration options related to the input files
#---------------------------------------------------------------------------
INPUT                  = "@PROJECT_SOURCE_DIR@/sdk/include/" \
                         "@PROJECT_SOURCE_DIR@/README.md" \
                         "@PROJECT_SOURCE_DIR@/doc/pages" \
                         "@PROJECT_SOURCE_DIR@/examples/README.md" \
                         "@PROJECT_SOURCE_DIR@/examples/data_visualizer/README.md" \
                         "@PROJECT_SOURCE_DIR@/examples/example_pixie16api/README.md"
INPUT_ENCODING         = UTF-8
FILE_PATTERNS          =
RECURSIVE              = YES
EXCLUDE                =
EXCLUDE_PATTERNS       = CMakeLists.txt
EXAMPLE_PATH           =
EXAMPLE_PATTERNS       = *
EXAMPLE_RECURSIVE      = NO
IMAGE_PATH             = "@PROJECT_SOURCE_DIR@/doc"
USE_MDFILE_AS_MAINPAGE = "@PROJECT_SOURCE_DIR@/README.md"
#---------------------------------------------------------------------------
# Configuration options related to source browsing
#---------------------------------------------------------------------------
VERBATIM_HEADERS       = NO
#---------------------------------------------------------------------------
# Configuration options related to the HTML output
#---------------------------------------------------------------------------
GENERATE_HTML          = YES
HTML_OUTPUT            = html
HTML_FILE_EXTENSION    = .html
HTML_HEADER            =
HTML_FOOTER            =
HTML_EXTRA_STYLESHEET  = "@PROJECT_SOURCE_DIR@/doc/css/doxygen-awesome/doxygen-awesome.css"
HTML_EXTRA_FILES       =
HTML_COLORSTYLE_HUE    = 208
HTML_COLORSTYLE_SAT    = 100
HTML_COLORSTYLE_GAMMA  = 100
HTML_DYNAMIC_SECTIONS  = YES
GENERATE_TREEVIEW      = NO
EXT_LINKS_IN_WINDOW    = YES
#---------------------------------------------------------------------------
# Configuration options related to the LaTeX output
#---------------------------------------------------------------------------
GENERATE_LATEX         = NO
#---------------------------------------------------------------------------
# Configuration options related to the RTF output
#---------------------------------------------------------------------------
GENERATE_RTF           = NO
#---------------------------------------------------------------------------
# Configuration options related to the man page output
#---------------------------------------------------------------------------
GENERATE_MAN           = NO
#---------------------------------------------------------------------------
# Configuration options related to the XML output
#---------------------------------------------------------------------------
GENERATE_XML           = NO
#---------------------------------------------------------------------------
# Configuration options related to the DOCBOOK output
#---------------------------------------------------------------------------
GENERATE_DOCBOOK       = NO
#---------------------------------------------------------------------------
# Configuration options for the AutoGen Definitions output
#---------------------------------------------------------------------------
GENERATE_AUTOGEN_DEF   = NO
#---------------------------------------------------------------------------
# Configuration options related to the Perl module output
#---------------------------------------------------------------------------
GENERATE_PERLMOD       = NO
#---------------------------------------------------------------------------
# Configuration options related to the preprocessor
#---------------------------------------------------------------------------
ENABLE_PREPROCESSING   = YES
MACRO_EXPANSION        = YES
#---------------------------------------------------------------------------
# Configuration options related to the dot tool
#---------------------------------------------------------------------------
CLASS_DIAGRAMS         = YES